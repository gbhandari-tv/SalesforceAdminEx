<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mid_Email_Check</fullName>
    <active>true</active>
    <errorConditionFormula>IF( CONTAINS(MID( Email , 5, 8),&apos;a&apos;),true,false)</errorConditionFormula>
    <errorDisplayField>Email</errorDisplayField>
    <errorMessage>Email does not contain &apos;a&apos; character between 5th and 12th character</errorMessage>
</ValidationRule>
